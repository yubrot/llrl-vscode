{
    "name": "llrl-vscode",
    "displayName": "llrl-vscode",
    "description": "llrl language support",
    "version": "0.0.1",
    "engines": {
        "vscode": "^1.53.0"
    },
    "categories": [
        "Programming Languages"
    ],
    "contributes": {
        "languages": [{
            "id": "llrl",
            "extensions": [".llrl"],
            "configuration": "./language-configuration.json"
        }],
        "grammars": [{
            "language": "llrl",
            "scopeName": "source.llrl",
            "path": "./syntaxes/llrl.tmLanguage.json"
        }]
    }
}
